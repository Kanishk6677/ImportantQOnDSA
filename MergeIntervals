// Question in the from of intervals
struct Intervals{
int s,e;
}
void mergeInterval(Intervals arr[],int n)
{
sort(arr,arr+n)
int index=0;
for(int i=1;i<n;i++)
{
if(arr[index].e>arr[i].s)
{
arr[index].e=max(arr[index].e,arr[i].e);
arr[index].s=max(arr[index].s,arr[i].s);
}
else
arr[index++]=arr[i];
}
}
