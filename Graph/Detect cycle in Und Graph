bool DFScycle(vector<int>adj[],int src,bool vis[],int parent)
{
vis[src]==true;
for(auto x:adj[src])
{
if(vis[x]==false)
if(DFScycle(adj,x,vis,src)==true)
return true;
else if(parent!=i) return true;
return false;
}



bool DFS(vector<int>adj[],int V)
 {
 bool vis[V]={false};
 for(int i=0;i<V;i++)
 {
 if(vis[i]==false)
 if(DFScycle(adj,i,vis,-1)==true) return true;
 return false;
 }
 
 
